#include <iostream>
using namespace std;

int main() {
    // Mendeklarasikan dan menginisialisasi variabel
    int x = 5, y = 6, z = 50;

    // Melakukan perhitungan
    int jumlah = x + y + z;          // Penjumlahan
    int selisih = z - x;             // Pengurangan
    int produk = x * y;              // Perkalian
    double pembagian = (double)z / (y + 1); // Pembagian

    // Menampilkan hasil
    cout << "Jumlah (x + y + z): " << jumlah << endl;
    cout << "Selisih (z - x): " << selisih << endl;
    cout << "Produk (x * y): " << produk << endl;
    cout << "Pembagian (z / (y + 1)): " << pembagian << endl;

    return 0;
}
